	mes[6] = "julio"; mes[7] = "agosto"; mes[8] = "septiembre"; mes[9] = "octubre"; mes[10] = "noviembre"; mes[11] = "diciembre";  // Asignar los nombres de los meses del año a los elementos del arreglo.
	
